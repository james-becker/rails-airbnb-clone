You are making a reservation for <%= @boat.name %>, the <%= @boat.category %>.
<br>
<br>
<%= simple_form_for :reservation, url: boat_reservations_path(@boat) do |f| %>
  <%= f.input :start_time, required: true, input_html: {"data-provide": 'datepicker'} %>
  <%= f.input :end_time, required: true, input_html: {"data-provide": 'datepicker'} %>
  <%= f.input :notes, hint: "Say hi to your host." %>
  <%= f.input :count, hint: "How many of you are staying?" %>
  <%= f.button :submit %>
<% end %>
